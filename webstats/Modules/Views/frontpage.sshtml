@Master['MasterPage']

@Section['Content']
<header class="container">
    <h1>@Model.Tournament</h1>
    <p>Welcome to @Model.Tournament betting scores</p>
</header>
<section class="row-alt">
<div class="container">
    <p><i>Results last updated @Model.TimeStamp</i></p>
    <h3>Betters</h3>
    @Partial['betters.sshtml']
</div>
</section>
<section class="row">
<div class="container">
    <h3>Groups</h3>
    @Partial['groups.sshtml', Model.Groups]
</div>
</section>
@EndSection