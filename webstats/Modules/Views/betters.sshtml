<div class="betters">
<table>
<tr>
    <th>Name</th>
    <th>Score</th>
    <th>Achievements</th>
</tr>
@Each.SortedBetters
<tr>
    <td><a href="@Current.Name">@Current.Name</a></td>
    <td>@Current.Score</td>
    <td class="achievement">@Current.AchievementsAsHtml</td>
</tr>
@EndEach
</table>
</div>
