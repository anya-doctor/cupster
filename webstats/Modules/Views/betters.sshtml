<div class="row">
<div class="col-md-5">
<table class="table table-bordered table-condensed">
<tr>
    <th>Name</th>
    <th>Score</th>
    <th><a href="legend">Achievements</a></th>
</tr>
@Each.SortedBetters
<tr class="@Current.RowClass">
    <td><a href="@Current.Name">@Current.Name</a></td>
    <td>@Current.Score</td>
    <td class="achievement">@Current.AchievementsAsHtml</td>
</tr>
@EndEach
</table>
</div>
</div>
